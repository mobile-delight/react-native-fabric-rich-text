{
  "name": "react-native-fabric-rich-text-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --buildFolder ios/build",
    "ios:release": "react-native run-ios --mode Release --buildFolder ios/build",
    "start": "react-native start",
    "test": "jest",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --mode Debug --buildFolder ios/build",
    "build:ios:e2e": "cd ios && FORCE_BUNDLING=1 xcodebuild -workspace FabricRichTextExample.xcworkspace -scheme FabricRichTextExample -configuration Debug -sdk iphonesimulator -derivedDataPath build build",
    "clean": "rm -rf android/app/build android/app/.cxx android/.gradle ../android/build ../android/.cxx ios/build ios/Pods ios/Podfile.lock && echo 'Example build artifacts cleaned'"
  },
  "dependencies": {
    "nativewind": "^4.2.0",
    "react": "19.2.0",
    "react-native": "0.83.2",
    "react-native-fabric-rich-text": "workspace:*",
    "react-native-reanimated": "^4.2.1",
    "react-native-worklets": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.2",
    "@react-native/metro-config": "0.83.2",
    "@react-native/typescript-config": "0.83.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.0",
    "jest": "^30.0.0",
    "react-native-builder-bob": "^0.40.17",
    "react-native-monorepo-config": "^0.3.1",
    "react-test-renderer": "19.2.0",
    "tailwindcss": "3.4.19"
  },
  "engines": {
    "node": ">=18"
  }
}
